{
  "manifest_version": 3,
  "name": "Gemini Ultimate Organizer - Folders/Prompts",
  "version": "2.4",
  "description": "Folders for Chats & Prompts, Export, Bulk Manager, Streamer Mode & Themes for Gemini. Ultimate Organizer.",
  "default_locale": "en",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://gemini.google.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://gemini.google.com/*"],
      "js": ["content_loader.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "config.js",
        "storage.js",
        "i18n.js",
        "ui.js",
        "main.js"
      ],
      "matches": ["https://gemini.google.com/*"]
    }
  ],
"browser_specific_settings": {
    "gecko": {
      "id": "gemini-organizer@ainstitor.com",
      "strict_min_version": "109.0",
      "data_collection_permissions": {
        "required": ["none"],
        "optional": []
      }
    }
  }
}